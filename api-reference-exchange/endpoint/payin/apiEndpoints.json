          {
            "name": "asset",
            "in": "path",
            "required": true,
            "description": "The crypto asset for which quotation is requested",
            "schema": {
              "type": "string",
              "example": "USDT"
            }
          },
          {
            "name": "network",
            "in": "path",
            "required": true,
            "description": "The network of the crypto asset",
            "schema": {
              "type": "string",
              "example": "BSC"
            }
          },
          {
            "name": "fiat",
            "in": "path",
            "required": true,
            "description": "The fiat currency in which the quotation is requested",
            "schema": {
              "type": "string",
              "example": "INR"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "Success"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "asset": {
                          "type": "string",
                          "example": "USDT"
                        },
                        "network": {
                          "type": "string",
                          "example": "BSC"
                        },
                        "fiat": {
                          "type": "string",
                          "example": "INR"
                        },
                        "rate": {
                          "type": "number",
                          "format": "decimal",
                          "example": 87.2
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
