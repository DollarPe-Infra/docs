                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      },
                      "example": {
                        "asset": ["This field is required."],
                        "amount": ["This field is required."],
                        "network": ["This field is required."]
                      }
                    }
                  }
                }
              }
            }
          },
