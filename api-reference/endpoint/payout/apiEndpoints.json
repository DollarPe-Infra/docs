                "asset": {
                  "type": "string",
                  "description": "The cryptocurrency asset code",
                  "example": "USDT"
                },
                "network": {
                  "type": "string",
                  "description": "The blockchain network",
                  "example": "BSC"
                },
                "amount": {
                  "type": "number",
                  "format": "decimal",
                  "description": "The amount to convert",
                  "example": 100.5
                }
              }
