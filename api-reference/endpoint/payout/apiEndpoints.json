                  "status": {
                    "type": "boolean",
                    "example": true
                  },
                  "message": {
                    "type": "string",
                    "example": "Success"
                  },
                  "data": {
                    "type": "object",
                    "properties": {
                      "asset": {
                        "type": "string",
                        "example": "USDT"
                      },
                      "network": {
                        "type": "string",
                        "example": "BSC"
                      },
                      "fiat": {
                        "type": "string",
                        "example": "INR"
                      },
                      "rate": {
                        "type": "number",
                        "format": "decimal",
                        "example": 87.2
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "400": {
          "description": "Bad Request",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "boolean",
                    "example": false
                  },
                  "message": {
                    "type": "string",
                    "example": "Bad Request"
                  },
                  "data": {
                    "type": "null"
                  },
                  "errors": {
                    "type": "object",
                    "additionalProperties": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "example": {
                      "asset": ["This field is required."],
                      "amount": ["This field is required."],
                      "network": ["This field is required."]
                    }
                  }
                }
              }
            }
          }
        },
        "500": {
          "description": "Internal Server Error",
